  "imports": {
    "#/*": {
      "development": "./src/*",
      "default": "./dist/*"
    }
  },
  "main": "index.js",
  "scripts": {
    "dev": "node --conditions=development --import tsx --watch ./src/index.ts",
    "typecheck": "tsc --noEmit",
