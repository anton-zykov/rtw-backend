    "start": "node dist/index.js",
    "test": "node --conditions=development --import tsx --test \"test/**/*.test.ts\""
  },
  "dependencies": {
    "fastify": "^5.6.1"
  },
  "devDependencies": {
    "@types/node": "^24.5.2",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2"
